<Window x:Class="Bot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Bot"
        mc:Ignorable="d"
        Title="BOT" Height="269.118" Width="369.118">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="94*"/>
            <ColumnDefinition Width="175*"/>
            <ColumnDefinition Width="94*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Grid.Column="1" Click="Button_Click">Start</Button>
        <TextBox  Grid.Row="1" Grid.Column="1" Name="Texxt"></TextBox>
        <Button Grid.Row="2" Grid.Column="1" Click="Navigate">Navigate</Button>
        <Button Grid.Row="4" Grid.Column="1" Click="Stop">Stop</Button>
    </Grid>
</Window>
